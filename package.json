{
    "name": "@1inch/fusion-sdk",
    "version": "2.1.11-rc.1",
    "description": "1inch Fusion SDK",
    "author": "@1inch",
    "private": false,
    "files": [
        "dist",
        "LICENSE",
        "README.md",
        "package.json"
    ],
    "module": "dist/esm/index.js",
    "main": "dist/cjs/index.js",
    "types": "dist/types/src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:1inch/fusion-sdk.git"
    },
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs && npm run build:types",
        "build:esm": "tsc --project tsconfig.esm.json",
        "build:cjs": "tsc --project tsconfig.cjs.json",
        "build:types": "tsc --project tsconfig.types.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint": "eslint  \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint:types": "tsc --noEmit",
        "test": "jest",
        "test:watch": "jest --watch",
        "changelog:generate": "changelog generate -a"
    },
    "dependencies": {
        "@1inch/byte-utils": "2.3.0",
        "@1inch/limit-order-sdk": "^4.9.3-rc.1",
        "ethers": "^6.13.1",
        "tslib": "^2.6.3",
        "ws": "^8.17.1"
    },
    "devDependencies": {
        "@1inch/eslint-config": "3.0.6",
        "@1inch/tsconfig": "1.0.8",
        "@swc/core": "1.5.25",
        "@swc/jest": "0.2.36",
        "@types/jest": "29.5.12",
        "@types/prompts": "^2.4.9",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^7.14.1",
        "@typescript-eslint/parser": "~5.51.0",
        "axios": "^1.7.2",
        "eslint": "9.27.0",
        "eslint-config-prettier": "~9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-import-resolver-typescript": "3.6.3",
        "eslint-plugin-import": "~2.30.0",
        "eslint-plugin-n": "^17.10.2",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^6.6.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "generate-changelog": "^1.8.0",
        "jest": "^29.7.0",
        "prettier": "^2.8.8",
        "ts-loader": "^9.5.1",
        "ts-mockito": "^2.6.1",
        "ts-node": "^10.9.2",
        "tslib": "2.6.3",
        "bluebird": "^3.7.2",
        "typescript": "4.9.4"
    },
    "peerDependencies": {
        "assert": "^2.0.0",
        "axios": ">=1 <2"
    },
    "peerDependenciesMeta": {
        "axios": {
            "optional": true
        },
        "assert": {
            "optional": true
        }
    },
    "volta": {
        "node": "18.16.0"
    },
