{
    "name": "@1inch/fusion-sdk",
    "version": "1.0.0",
    "description": "1inch Fusion Mode SDK",
    "author": "@1inch",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "LICENSE",
        "README.md",
        "package.json"
    ],
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:1inch/fusion-sdk.git"
    },
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint": "eslint  \"{src,apps,libs,test}/**/*.ts\" --ignore-path .eslintignore --fix",
        "lint:types": "tsc --noEmit",
        "test": "jest",
        "test:watch": "jest --watch",
        "changelog:generate": "changelog generate -a"
    },
    "dependencies": {
        "@1inch/limit-order-sdk": "4.0.1",
        "@metamask/eth-sig-util": "^5.0.2",
        "bn.js": "^5.2.1",
        "ethers": "6.11.0",
        "tslib": "^2.2.0",
        "ws": "^8.12.1"
    },
    "devDependencies": {
        "@1inch/tsconfig": "1.0.7",
        "@1inch/eslint-config": "1.4.3",
        "@babel/core": "^7.13.16",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-typescript": "^7.13.0",
        "@types/jest": "^26.0.22",
        "@types/prompts": "^2.0.12",
        "@types/ws": "^8.5.4",
        "axios": "^1.6.7",
        "babel-jest": "^26.6.3",
        "@typescript-eslint/eslint-plugin": "5.59",
        "@typescript-eslint/parser": "5.51",
        "eslint": "8.41.0",
        "eslint-config-prettier": "8.3",
        "eslint-config-standard": "17",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-import": "2.26",
        "eslint-plugin-n": "16",
        "eslint-plugin-prettier": "4",
        "eslint-plugin-promise": "6",
        "eslint-plugin-unused-imports": "2",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "ts-loader": "^9.0.2",
        "ts-mockito": "^2.6.1",
        "ts-node": "^10.4.0",
        "typescript": "4.9.4"
    },
    "peerDependencies": {
        "assert": "^2.0.0",
        "axios": ">=1 <2",
        "stream": "^0.0.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && yarn run typecheck"
        }
    },
    "lint-staged": {
        "*.{js,ts,md,json}": [
            "yarn run prettier"
        ],
        "*.{js,ts}": [
            "yarn run lint"
        ]
    },
    "volta": {
        "node": "18.16.0",
        "yarn": "1.22.17"
    }
}
