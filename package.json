{
    "name": "@1inch/fusion-sdk",
    "version": "2.2.3-rc.1",
    "description": "1inch Fusion SDK",
    "author": "@1inch",
    "files": [
        "dist",
        "LICENSE",
        "README.md",
        "package.json"
    ],
    "module": "./dist/esm/index.js",
    "main": "./dist/cjs/index.js",
    "types": "./dist/types/src/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/src/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "node": "./dist/cjs/index.js",
            "default": "./dist/cjs/index.js"
        },
        "./api": {
            "types": "./dist/types/src/api-ext.d.ts",
            "import": "./dist/esm/api-ext.js",
            "require": "./dist/cjs/api-ext.js",
            "node": "./dist/cjs/api-ext.js",
            "default": "./dist/cjs/api-ext.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:1inch/fusion-sdk.git"
    },
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs && npm run build:types",
        "build:esm": "tsc --project tsconfig.esm.json",
        "build:cjs": "tsc --project tsconfig.cjs.json",
        "build:types": "tsc --project tsconfig.types.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint": "eslint  \"{src,apps,libs,test}/**/*.ts\"",
        "lint:types": "tsc --noEmit",
        "test": "TZ=UTC jest",
        "test:integration": "TZ=UTC forge build && jest -c jest.integration.config.js",
        "test:watch": "jest --watch",
        "changelog:generate": "changelog generate -a"
    },
    "dependencies": {
        "@1inch/byte-utils": "3",
        "@1inch/limit-order-sdk": "4.12.1",
        "ethers": "^6.13.5",
        "tslib": "^2.8.1",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@1inch/eslint-config": "3.0.6",
        "@1inch/tsconfig": "1.0.8",
        "@swc/core": "1.10.9",
        "@swc/jest": "0.2.37",
        "@types/jest": "29.5.14",
        "@types/prompts": "^2.4.9",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^8.21.0",
        "@typescript-eslint/parser": "^8.21.0",
        "axios": "^1.7.2",
        "eslint": "~9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-config-standard": "^17.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-n": "^17.10.2",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "generate-changelog": "^1.8.0",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "testcontainers": "10.17.1",
        "ts-loader": "^9.5.1",
        "ts-mockito": "^2.6.1",
        "ts-node": "^10.9.2",
        "tslib": "2.6.3",
        "typescript": "5.7.3"
    },
    "peerDependencies": {
        "assert": "^2.0.0",
        "axios": ">=1 <2"
    },
    "peerDependenciesMeta": {
        "axios": {
            "optional": true
        },
        "assert": {
            "optional": true
        }
    },
    "volta": {
        "node": "22.13.1"
    },
    "pnpm": {
        "overrides": {
            "axios@>=1.3.2 <=1.7.3": ">=1.7.4",
            "micromatch@<4.0.8": ">=4.0.8",
            "webpack@>=5.0.0-alpha.0 <5.94.0": ">=5.94.0",
            "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5"
        }
    }
}
